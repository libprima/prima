        401     1      call assert(.not. any(is_nan(fhist(1:min(nf, maxfhist))) .or. is_posinf(fhist(1:min(nf, maxfhist)))), &
        402     1          & 'FHIST does not contain NaN/+Inf', srname)
        403     2      if (present(chist)) then
