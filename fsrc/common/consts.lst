        179     1        ELSE
        180     1            TEMPD=DSIGN(HALFRT*RHO,DLIN*(GHG+VHV))
        181     1            TEMPV=DSIGN(HALFRT*RHO,VLIN*(GHG+VHV))
        182     1        END IF
        183     1        DO I=1,N
        184     1            D(I)=TEMPD*D(I)+TEMPV*V(I)
        185     1        END DO
